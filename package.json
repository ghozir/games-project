{
  "name": "codebase",
  "version": "1.0.0",
  "description": "main codebase backend codeviz",
  "main": "index.js",
  "engines": {
    "node": "22.11"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "junit": "_mocha \"test/**/*_test.js\" --timeout=5000 --reporter mocha-junit-reporter",
    "test": "mocha \"test/**/*_test.js\" --timeout=5000 --exit",
    "cover": "nyc _mocha \"test/**/*_test.js\" --recursive --exit",
    "lint": "eslint bin test",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghozir/oceaneyes-be.git"
  },
  "keywords": [
    "codebase",
    "api",
    "backend",
    "nodejs"
  ],
  "author": "Codex Viz",
  "license": "(ISC OR GPL-3.0)",
  "dependencies": {
    "argon2": "^0.41.1",
    "aws-sdk": "^2.304.0",
    "axios": "^0.28.1",
    "confidence": "^5.0.0",
    "date-fns": "^2.17.0",
    "dayjs": "^1.10.4",
    "dd-trace": "^3.0.0",
    "dotenv": "^8.1.0",
    "events": "^3.2.0",
    "firebase-admin": "^12.3.1",
    "hashids": "^2.2.8",
    "hot-shots": "^9.2.0",
    "joi": "^17.1.1",
    "jsftp": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "minio": "^7.0.18",
    "mongodb": "^3.6.3",
    "morgan": "^1.10.0",
    "mysql": "^2.15.0",
    "nodemon": "^3.1.4",
    "passport": "^0.7.0",
    "passport-http": "0.3.0",
    "pg": "^8.5.1",
    "query-string": "^6.13.8",
    "randomstring": "^1.3.0",
    "redis": "^3.0.2",
    "restify": "^11.1.0",
    "restify-cors-middleware2": "^2.2.1",
    "rewire": "^5.0.0",
    "util": "^0.12.3",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1",
    "winston": "^3.3.3",
    "winston-logstash": "^0.4.0",
    "winston3-logstash-transport": "^1.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^10.7.3",
    "mocha-junit-reporter": "^2.2.1",
    "nyc": "^15.0.1",
    "pre-commit": "^1.0.10",
    "sinon": "^4.2.2"
  },
  "nyc": {
    "lines": 10,
    "statements": 10,
    "functions": 10,
    "branches": 10,
    "check-coverage": true,
    "include": [
      "bin/**/*.js"
    ],
    "exclude": [
      "bin/app/**/*.js"
    ],
    "reporter": [
      "html",
      "text",
      "lcov",
      "text-summary"
    ],
    "all": true,
    "cache": true,
    "report-dir": "./coverage"
  },
  "pre-commit": []
}
